    "version": "0.2.0",
    "configurations": [
        {
            "name": "Crime GPS to Zillow Neighborhood",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\01_DataPreProcessing\\Crime_GPS_to_Zillow_Neighborhood\\Crime_GPS_to_Zillow_Neighborhood.py",
            "console": "integratedTerminal",
            "args": [
                "-ic",
                "${workspaceFolder}\\datasets\\ChicagoTrimmedCrimeData.csv",
                "-is",
                "${workspaceFolder}\\01_DataPreProcessing\\Crime_GPS_to_Zillow_Neighborhood\\ZillowNeigborhoods\\zillow-neighborhoods"
            ]
        },
        {
            "name": "ZHVI Row Transform",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\01_DataPreProcessing\\ZHVI_row_tranform\\ZHVI_to_row_tranformation.py",
            "console": "integratedTerminal",
            "args": [
                "-i",
                "${workspaceFolder}\\datasets\\ChicagoTrimmedHousingData.csv",
                "-ftr",
                "2001-01",
                "-o",
                "ChicagoTrimmedHousingData_transformed_2001AndBeyond.csv"
            ]
        },
        {
            "name": "DNN_Data_Creator",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\01_DataPreProcessing\\PastFutureDataCombinerForDNN\\PastMonthsToTrain_FutureZHVITarget_Generator.py",
            "console": "integratedTerminal",
            "args": [
                "-i",
                "${workspaceFolder}\\datasets\\Train\\TrainData.csv",
                "-pm",
                "6",
                "-fm",
                "3"
            ]
        },
        {
            "name": "Train Model",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\02_ModelDefintion\\training.py",
            "console": "integratedTerminal",
            "args": [
                "-i",
                "${workspaceFolder}\\datasets\\Train\\TrainData_housingCrime_4Prev_2Future_Months.csv",
                // "-pm",
                // "4",
                // "-fm",
                // "2",
                "--output_model_name",
                "experiments\\ConeModel",
                "-gpu",
                "--epochs",
                "3"
            ]
        },
        {
            "name": "Evaluate Model",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\03_Model_Evaluation\\inference.py",
            "console": "integratedTerminal",
            "args": [
                "-m",
                "${workspaceFolder}\\experiments\\simpleFirstModelTest\\model_and_best_wieghts.hdf5",
                "-i",
                "${workspaceFolder}\\datasets\\Train\\TrainData_housingCrime_4Prev_2Future_Months.csv",
                "-gpu"
            ]
        }
    ]
}